package sort;

class Counter {

	int counter1 = 0;

	public void incrementCounter1() {
		this.counter1 = this.counter1 + 1;
	}
	
	public void decrementCounter1() {
		this.counter1 = this.counter1 - 1;
	}
	
	public int getCounter1() {
		return this.counter1;
	}
	
	public void addCounter1(Counter c) {
		this.counter1 = this.counter1 + c.getCounter1();
	}
	
	public void displayCounter1() {
		System.out.println(this.counter1);
	}
}